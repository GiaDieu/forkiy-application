{
  "name": "forkify",
  "version": "1.0.0",
  "description": "Recipe Application",
  "default": "index.html",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --no-minify --public-url ./dist",
    "clean:builder": "rm -rf ./dist",
    "clean:cache": "rm -rf  ./.cache",
    "clean": "npm run clean:builder && npm run clean:cache"
  },
