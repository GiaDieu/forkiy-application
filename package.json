  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "parcel-bundler": "^1.12.5",
    "sass": "^1.34.1"
  },
  "dependencies": {
    "core-js": "^3.14.0",
    "fractional": "^1.0.0",
    "regenerator-runtime": "^0.13.7"
  }
}
